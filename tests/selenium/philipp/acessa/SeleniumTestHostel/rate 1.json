{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://wwwdev.ondeficar.com/auth/login"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "text": "selenium@cloudbeds.com"
    },
    {
      "type": "sendKeysToElement",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "testTime!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='form-actions']//button[normalize-space(.)='Login']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "arates"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Best Available Rate"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='pull-line-right']//a[.=' Save']"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Rates / Room Rates"
    },
    {
      "type": "waitForTextPresent",
      "text": "Saved"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}