{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://wwwdev3.ondeficar.com/auth/logout"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "text": "selenium@cloudbeds.com"
    },
    {
      "type": "sendKeysToElement",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "\\undefined"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "testTime!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='form-actions']//button[normalize-space(.)='Login']"
      }
    },
    {
      "type": "waitForCurrentUrl",
      "url": "http://wwwdev3.ondeficar.com/connect/366#/dashboard"
    },
    {
      "type": "waitForText",
      "locator": {
        "type": "css selector",
        "value": ".page-title"
      },
      "text": "Dashboard"
    },
    {
      "type": "get",
      "url": "http://wwwdev3.ondeficar.com/connect/366#/inventory"
    },
    {
      "type": "waitForText",
      "locator": {
        "type": "css selector",
        "value": ".page-title"
      },
      "text": "Inventory Allocation"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": ".invRoom .sum_rooms"
      },
      "text": "1000"
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='layout_container']/div[21]"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}