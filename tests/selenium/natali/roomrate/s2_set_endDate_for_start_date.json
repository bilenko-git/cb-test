{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a[href=\"#tab_0\"]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#tab_0 .add_interval"
      }
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "#tab_0 .new_interval_form:not(.hide)"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": ".new_interval_form i.icon-calendar"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "td.ui-datepicker-days-cell-over.ui-datepicker-today a"
      }
    },
    {
      "type": "storeElementValue",
      "locator": {
        "type": "name",
        "value": "start_date"
      },
      "variable": "start_date"
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "name",
        "value": "end_date"
      },
      "value": "${start_date}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a.btn.cancel_add_interval"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}