{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "li.base.active > a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='tab_0']/form/div[6]/div/div[2]/table/tbody/tr[4]/td[7]/a[1]/i"
      }
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "#tab_0 .new_interval_form:not(.hide)"
      }
    },
    {
      "type": "store",
      "text": "Selenium Interval",
      "variable": "new_interval_name"
    },
    {
      "type": "storeElementValue",
      "locator": {
        "type": "name",
        "value": "interval_name"
      },
      "variable": "old_interval_name"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "interval_name"
      },
      "text": "${new_interval_name}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#tab_0  .save_add_interval"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "${new_interval_name}"
    },
    {
      "type": "assertText",
      "locator": {
        "type": "css selector",
        "value": "#tab_0 .intervals-table tr:not(.clonable):not(.empty) .interval_name .interval_text"
      },
      "text": "${new_interval_name}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='pull-line-right']//a[.=' Save']"
      }
    },
    {
      "type": "waitForElementAttribute",
      "locator": {
        "type": "css selector",
        "value": ".savingMsg"
      },
      "attributeName": "data-qe-id",
      "value": "saved"
    },
    {
      "type": "refresh",
      "step_name": "Refresh page and check that we have new interval name"
    },
    {
      "type": "print",
      "text": "${new_interval_name}"
    },
    {
      "type": "print",
      "text": "${old_interval_name}"
    },
    {
      "type": "assertText",
      "locator": {
        "type": "css selector",
        "value": "#tab_0 .intervals-table tr:not(.clonable):not(.empty) .interval_name .interval_text"
      },
      "text": "${new_interval_name}"
    },
    {
      "type": "clickElement",
      "step_name": "Try to restore old interval name",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='tab_0']/form/div[6]/div/div[2]/table/tbody/tr[4]/td[7]/a[1]/i"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "interval_name"
      },
      "text": "${old_interval_name}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#tab_0  .save_add_interval"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='pull-line-right']//a[.=' Save']"
      }
    },
    {
      "type": "waitForElementAttribute",
      "locator": {
        "type": "css selector",
        "value": ".savingMsg"
      },
      "attributeName": "data-qe-id",
      "value": "saved"
    },
    {
      "type": "refresh"
    },
    {
      "type": "assertText",
      "step_name": "Verify old interval name after refresh",
      "locator": {
        "type": "css selector",
        "value": "#tab_0 .intervals-table tr:not(.clonable):not(.empty) .interval_name .interval_text"
      },
      "text": "${old_interval_name}"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}