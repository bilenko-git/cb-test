{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "store",
      "text": "roomRates",
      "variable": "hash"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "arates"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Best Available Rate"
      }
    },
    {
      "type": "waitForElementAttribute",
      "locator": {
        "type": "id",
        "value": "layout"
      },
      "attributeName": "data-current_view",
      "value": "${hash}"
    },
    {
      "type": "assertElementAttribute",
      "locator": {
        "type": "id",
        "value": "layout"
      },
      "attributeName": "data-current_view",
      "value": "${hash}"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}