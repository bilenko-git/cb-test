{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://wwwdev.ondeficar.com/auth/login"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "text": "selenium@cloudbeds.com"
    },
    {
      "type": "sendKeysToElement",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "testTime!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='form-actions']//button[normalize-space(.)='Login']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#ssettings > a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#sprofile1 > a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Hotel Profile"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Property Profile"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "hotel_name"
      },
      "text": "SeleniumTest Hotel"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "hotel_phone"
      },
      "text": "(111)555-8899"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "hotel_contact_name"
      },
      "text": "Selenium hotelier"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "hotel_address1"
      },
      "text": "1 selenium st. Good City"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "hotel_address_city"
      },
      "text": "California"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "hotel_address_zip"
      },
      "text": ""
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='pull-line-right']//a[.=' Save']"
      }
    },
    {
      "type": "waitForText",
      "locator": {
        "type": "xpath",
        "value": "/html/body/div[3]/div[2]/div[2]/div/div[2]/div[2]/div[21]/div[2]"
      },
      "text": "ZIP: This field is required."
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='footer-bottom-wrapper']//button[.='Ok']"
      }
    },
    {
      "type": "assertElementAttribute",
      "locator": {
        "type": "id",
        "value": "hotel_address_zip"
      },
      "attributeName": "class",
      "value": "m-wrap span12 account_profile required inputLim_Ignore error"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}