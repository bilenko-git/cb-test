    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "room_type_private"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a.pluse > img"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "room_type_private"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#tab_2 > form > div.row-fluid > div.span12.roomTypePrivate > div.control-group.span6 > div.controls > label.radio.label_radio > #uniform-undefined > span.checked > input[name=\"room_type_private\"]"
