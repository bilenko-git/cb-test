{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://wwwdev.ondeficar.com/auth/login"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "text": "selenium@cloudbeds.com"
    },
    {
      "type": "sendKeysToElement",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "testTime!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='form-actions']//button[normalize-space(.)='Login']"
      }
    },
    {
      "type": "get",
      "url": "http://wwwdev.ondeficar.com/connect/366#/roomTypes"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": ".pluse > img"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "label_8"
      },
      "text": "Alona test"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "label_9"
      },
      "text": "AS"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='tab_2']/form/div[3]/div[1]/div[1]/div/select//option[4]"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Persons can not be larger than the maximum occupancy of room"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='layout_container']//button[.='Ok']"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}