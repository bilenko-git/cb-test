{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://wwwdev.ondeficar.com/auth/login"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "text": "selenium@cloudbeds.com"
    },
    {
      "type": "sendKeysToElement",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "testTime!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='form-actions']//button[normalize-space(.)='Login']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#ssettings > a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#saccommodations > a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Inventory Allocation"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "/html/body/div[3]/div[2]/div[2]/div/div[2]/div[2]/div[2]/div/div[2]/div/form/div[6]/table/tbody/tr[3]/td[2]/input"
      },
      "text": "10"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "/html/body/div[3]/div[2]/div[2]/div/div[2]/div[2]/div[2]/div/div[2]/div/form/div[6]/table/tbody/tr[3]/td[3]/input"
      },
      "text": "8"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='pull-line-right']//a[.=' Save']"
      }
    },
    {
      "type": "waitForText",
      "locator": {
        "type": "xpath",
        "value": "/html/body/div[3]/div[2]/div[2]/div/div[2]/div[3]/div[1]/div/div"
      },
      "text": "Saved"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}